/* free/free.wxss */
.head-img{
	width: 100%;
}

.button{
	margin: 80rpx 20rpx;
}


.section{
	font-size: 28rpx;
	padding: 7px 15px;
	display: flex;
}

.sections{
	margin-right: 20px;
	color: #666;
}

.uploadCd>view {
	text-align: center;
	color: #4E565F
}
.circleWhite {
	width: 86rpx;
	height: 86rpx;
	border-radius: 50%;
	background: #fff;
	display: flex;
	justify-content: center;
	align-items: center
}
.circleWhite>image {
	width: 38rpx;
	height: 38rpx;
}

.shenfenWord {
	font-size: 15px;
	margin-right: 18px
}
.uploadCd {
	padding: 24px 15px 10px;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.set{
	width: 330rpx;
	height: 330rpx;
}

.uploadCd>view>view {
	width:330rpx;
	height:208rpx;
	background:rgba(239,241,244,1);
	border-radius:2px;
	display: flex;
	justify-content: center;
	align-items: center;
	margin-bottom: 30rpx;
}

.picker{
	width: 400rpx;
}

/* miniprogram/pages/complete.wxss */

.complete {
	margin: 124rpx 70rpx;
}

.label {
	border-bottom: 1rpx dashed #eee;
	padding-bottom: 30rpx;
}

.placeholder {
	font-style: 26rpx;
}

.code {
	margin-top: 60rpx;
	display: flex;
	justify-content: space-between;
}

.code input {
	width: 300rpx;
}

.code-button {
	margin: 0;
	width: 200rpx;
	height: 56rpx;
	line-height: 52rpx;
	padding: 0 10rpx;
	font-size: 26rpx;
	background: transparent;
	color: #448ff6;
	border: 2rpx solid rgba(68, 142, 246, 1);
}

.sure {
	margin-top: 126rpx;
	width: 610rpx;
	height: 88rpx;
	background: rgba(68, 142, 246, 1);
	border-radius: 4rpx;
	color: white;
}

.active {
	color: #999;
	border-color: #999;
}

input{
	flex:1
}

